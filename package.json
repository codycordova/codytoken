{
  "name": "cody-token-web3",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL-3.0",
  "packageManager": "npm@11.6.0",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && tsc -p src/ws/tsconfig.json",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "ci:stellar-validate": "node scripts/stellar-validate.mjs",
    "ci:verify": "npm run lint && npm run typecheck && npm run ci:stellar-validate && npm run build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:strict": "npm audit --audit-level=high",
    "security:fix": "npm audit fix --force",
    "security:update": "npm update && npm audit fix",
    "security:check": "npm audit && npm outdated",
    "ci:install:locked": "npm ci --ignore-scripts && npm audit --audit-level=moderate"
  },
  "dependencies": {
    "@creit.tech/stellar-wallets-kit": "^0.8.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@sentry/nextjs": "^9.39.0",
    "@stellar/stellar-sdk": "^13.3.0",
    "decimal.js": "^10.6.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "next": "^15.3.3",
    "node-fetch": "^2.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "three": "^0.179.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2"
  }
}
